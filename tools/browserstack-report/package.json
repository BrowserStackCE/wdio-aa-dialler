{
  "name": "browserstack-reporting-tools",
  "version": "0.1.0",
  "private": true,
  "description": "BrowserStack reporting generator (CSV/XLSX/Markdown/JSON)",
  "scripts": {
    "report": "tsx generate-browserstack-report.ts --config browserstack-report.config.json",
    "report:sample": "tsx generate-browserstack-report.ts --config browserstack-report.config.sample.json",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
